<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.FXCollections?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.lang.String?>
<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.mikemillar.basicroutesheet.DialogController"
            prefHeight="400.0" prefWidth="1400" maxHeight="400">
    <content>
        <HBox>
            <GridPane>
                <Label text="RO Number" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
                <TextField fx:id="roNumField" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
                <Label text="Tag Number" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
                <TextField fx:id="tagNumField" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
                <Label text="Year" GridPane.rowIndex="0" GridPane.columnIndex="2"/>
                <TextField fx:id="yearField" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
                <Label text="Make" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
                <TextField fx:id="makeField" GridPane.rowIndex="1" GridPane.columnIndex="3"/>
                <Label text="Model" GridPane.rowIndex="0" GridPane.columnIndex="4"/>
                <TextField fx:id="modelField" GridPane.rowIndex="1" GridPane.columnIndex="4"/>
                <Label text="Mileage" GridPane.rowIndex="0" GridPane.columnIndex="5"/>
                <TextField fx:id="mileField" GridPane.rowIndex="1" GridPane.columnIndex="5"/>
                <Label text="Customer Name" GridPane.rowIndex="0" GridPane.columnIndex="6"/>
                <TextField fx:id="customerNameField" GridPane.rowIndex="1" GridPane.columnIndex="6"/>
                <Label text="Phone Number" GridPane.rowIndex="0" GridPane.columnIndex="7"/>
                <TextField fx:id="phoneNumField" GridPane.rowIndex="1" GridPane.columnIndex="7"/>
                <Label text="Jobs" GridPane.rowIndex="0" GridPane.columnIndex="8"/>
                <TextField fx:id="jobsField" GridPane.rowIndex="1" GridPane.columnIndex="8"/>
                <Label text="Adviser" GridPane.rowIndex="0" GridPane.columnIndex="9"/>
                <TextField fx:id="adviserField" GridPane.rowIndex="1" GridPane.columnIndex="9"/>
                <Label text="Tech" GridPane.rowIndex="0" GridPane.columnIndex="10"/>
                <TextField fx:id="techField" GridPane.rowIndex="1" GridPane.columnIndex="10"/>
                <Label text="Time Due" GridPane.rowIndex="0" GridPane.columnIndex="11"/>
                <ComboBox fx:id="dueTime" GridPane.rowIndex="1" GridPane.columnIndex="11">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="8:00 AM"/>
                            <String fx:value="8:15 AM"/>
                            <String fx:value="8:30 AM"/>
                            <String fx:value="8:45 AM"/>
                            <String fx:value="9:00 AM"/>
                            <String fx:value="9:15 AM"/>
                            <String fx:value="9:30 AM"/>
                            <String fx:value="9:45 AM"/>
                            <String fx:value="10:00 AM"/>
                            <String fx:value="10:15 AM"/>
                            <String fx:value="10:30 AM"/>
                            <String fx:value="10:45 AM"/>
                            <String fx:value="11:00 AM"/>
                            <String fx:value="11:15 AM"/>
                            <String fx:value="11:30 AM"/>
                            <String fx:value="11:45 AM"/>
                            <String fx:value="12:00 PM"/>
                            <String fx:value="12:15 PM"/>
                            <String fx:value="12:30 PM"/>
                            <String fx:value="12:45 PM"/>
                            <String fx:value="1:00 PM"/>
                            <String fx:value="1:15 PM"/>
                            <String fx:value="1:30 PM"/>
                            <String fx:value="1:45 PM"/>
                            <String fx:value="2:00 PM"/>
                            <String fx:value="2:15 PM"/>
                            <String fx:value="2:30 PM"/>
                            <String fx:value="2:45 PM"/>
                            <String fx:value="3:00 PM"/>
                            <String fx:value="3:15 PM"/>
                            <String fx:value="3:30 PM"/>
                            <String fx:value="3:45 PM"/>
                            <String fx:value="4:00 PM"/>
                            <String fx:value="4:15 PM"/>
                            <String fx:value="4:30 PM"/>
                            <String fx:value="4:45 PM"/>
                            <String fx:value="5:00 PM"/>
                            <String fx:value="5:15 PM"/>
                            <String fx:value="5:30 PM"/>
                            <String fx:value="5:45 PM"/>
                            <String fx:value="6:00 PM"/>
                        </FXCollections>
                    </items>
                </ComboBox>
                <Label text="Waiting?" GridPane.rowIndex="0" GridPane.columnIndex="12"/>
                <ComboBox fx:id="waiterField" GridPane.rowIndex="1" GridPane.columnIndex="12">
                    <items>
                        <FXCollections fx:factory="observableArrayList">
                            <String fx:value="Yes"/>
                            <String fx:value="No"/>
                        </FXCollections>
                    </items>
                </ComboBox>
                <Label text="Notes" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
                <TextArea fx:id="notesField" GridPane.rowIndex="3" GridPane.columnIndex="0" GridPane.columnSpan="13"/>
            </GridPane>
        </HBox>
    </content>

</DialogPane>
