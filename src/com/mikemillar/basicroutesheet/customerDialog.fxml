<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.mikemillar.basicroutesheet.DialogController">
    <content>
        <GridPane hgap="10" vgap="10">
            <Label text="Customer Name" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="updateCustomerName" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <Label text="Customer Phone Number" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <TextField fx:id="updateCustomerNumber" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
        </GridPane>
    </content>
</DialogPane>
