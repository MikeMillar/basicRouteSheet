<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.mikemillar.basicroutesheet.DialogController">
    <content>
        <GridPane hgap="10" vgap="10">
            <Label text="Year" GridPane.rowIndex="0" GridPane.columnIndex="0"/>
            <TextField fx:id="updateYear" GridPane.rowIndex="1" GridPane.columnIndex="0"/>
            <Label text="Make" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
            <TextField fx:id="updateMake" GridPane.rowIndex="1" GridPane.columnIndex="1"/>
            <Label text="Model" GridPane.rowIndex="0" GridPane.columnIndex="2"/>
            <TextField fx:id="updateModel" GridPane.rowIndex="1" GridPane.columnIndex="2"/>
            <Label text="Mileage" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
            <TextField fx:id="updateMileage" GridPane.rowIndex="1" GridPane.columnIndex="3"/>
        </GridPane>
    </content>
</DialogPane>
